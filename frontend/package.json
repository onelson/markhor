{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.5",
    "immutable": "^3.8.1",
    "keymirror": "^0.1.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "semantic-ui-less": "^2.2.12",
    "semantic-ui-react": "^0.71.5"
  },
  "scripts": {
    "build-css": "lessc --include-path=./node_modules/semantic-ui-less/ src/index.less src/index.css",
    "watch-css": "chokidar ./theme.config src/**/*.less --initial -c \"npm run build-css\" ",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "chokidar-cli": "^1.2.0",
    "less": "^2.7.2",
    "npm-run-all": "^4.0.2",
    "react-scripts": "1.0.11",
    "redux-devtools-extension": "^2.13.2"
  },
  "proxy": "http://localhost:8000"
}
